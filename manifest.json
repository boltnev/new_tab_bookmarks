{
  "manifest_version": 2,

  "name": "Improved start page",
  "description": "This extension allows to add more bookmarks on start page",
  "version": "1.0",

  "chrome_url_overrides" : {
    "newtab": "my_newtab.html"
  },
  
  "permissions": [
    "bookmarks",
	"history",
	"management",
	"chrome://favicon/"
  ],
  
  "content_security_policy": "object-src 'self'; script-src 'self' 'unsafe-eval'"
  
}
